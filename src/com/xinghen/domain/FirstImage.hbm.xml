<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.xinghen.domain">

	<class name="FirstImage" table="first_image">
	
		<id name="id">
            <generator class="native"/>
		</id>
		
		<property name="imageName"></property>
		<property name="uploadDate"></property>
		<property name="imageType"></property>
		
		<many-to-one name="usedGood" column="usedGoodId" class = "UsedGood" unique="true"></many-to-one>
		
	</class>
	
</hibernate-mapping>
